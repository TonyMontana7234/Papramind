# Papramind - Minimal Working Configuration
# Replace these with your actual API keys

# Essential for basic functionality
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=pk_test_your_clerk_publishable_key_here
CLERK_SECRET_KEY=sk_test_your_clerk_secret_key_here

# Database (Railway will provide DATABASE_URL automatically)
DATABASE_URL=postgresql://postgres:password@localhost:5432/papramind
DB_HOST=localhost
DB_PORT=5432
DB_NAME=papramind
DB_USER=postgres
DB_PASSWORD=password123

# Security (generate random 32+ character strings)
JWT_SECRET=your_jwt_secret_here_minimum_32_characters_long
ENCRYPTION_KEY=your_encryption_key_here_minimum_32_characters_long

# Optional APIs (for full functionality)
OPENAI_API_KEY=sk-your_openai_api_key_here
PINECONE_API_KEY=your_pinecone_api_key_here
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
AWS_S3_BUCKET=your_s3_bucket_name_here

# Application
NEXT_PUBLIC_APP_URL=http://localhost:3000
NODE_ENV=development